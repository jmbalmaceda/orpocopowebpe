<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="net.technisys.assets.reportCenter.model.TemplateJasper" table="TEMPLATE_JASPER" >
        <id name="templateJasperId" type="java.lang.Long" column="TEMPLATE_JASPER_ID">
        	<generator class="native">
        		<param name="sequence">SEQ_TEMPLATE_JASPER</param>
        	</generator>
        </id>
        <property name="templateId" column="ID_TEMPLATE" />
        <property name="actor" column="S_ACTOR" />
        <property name="country" column="S_COUNTRY" />
        <property name="device" column="S_DEVICE" />
        <property name="error" column="B_ERROR" />
        <property name="servicio" column="S_SERVICIO" />
        <property name="solution" column="S_SOLUTION" />
        <property name="segment" column="S_SEGMENT" />
    </class>
</hibernate-mapping>