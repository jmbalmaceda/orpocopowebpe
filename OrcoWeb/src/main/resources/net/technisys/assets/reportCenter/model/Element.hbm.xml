<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="net.technisys.assets.reportCenter.model.Element" table="ELEMENT" >
        
               
        <id name="elementId" type="java.lang.Long" column="ID_ELEMENT">
        	<generator class="native">
        		<param name="sequence">SEQ_ELEMENT</param>
        	</generator>
        </id>
        
        <map name="elementData" table="ELEMENT_DATA" cascade="all" lazy="false">
        	<key column="N_ID_ELEMENT" />
        	<map-key type="string" column="S_KEY" />
        	<element type="string" column="S_VALUE"/>
        </map>
        <property name="actor" column="S_ACTOR" />
        <property name="country" column="S_COUNTRY" />
        <property name="device" column="S_DEVICE" />
        <property name="elementName" column="S_ELEMENT_NAME" />
        <property name="error" column="B_ERROR" />
        <property name="servicio" column="S_SERVICIO" />
        <property name="solution" column="S_SOLUTION" />
        <property name="xPath" column="S_XPATH" />
        <property name="type" column="S_TYPE" />
        <property name="segmento" column="S_SEGMENT" />
        <property name="variante" column="S_VARIANTE" />
        <property name="isField" column="B_IS_FIELD" />
        <property name="isIterable" column="B_IS_ITERABLE" />
        <property name="description" column="S_DESCRIPTION" />
    </class>
</hibernate-mapping>